<SiconosModel>
	<NSDS bvp='false'>
		<!-- DSs defined in the problem -->
		<DS_Definition>		
			<LagrangianLinearTIDS number='3' Id="Ball">
	 			<StepsInMemory>1</StepsInMemory>				
				<q0 vectorSize='3'>
					1.0 0.0 0.0
				</q0>
				<Velocity0  vectorSize='3'>
					0.0 0.0 0.0
				</Velocity0>
				<FExt vectorPlugin="TestPlugin:computeFExt"/>
				<Mass matrixRowSize='3' matrixColSize='3'>	
					<row>1.0 0.0 0.0</row>
					<row>0.0 1.0 0.0</row>
					<row>0.0 0.0 1.0</row>
				</Mass>			
			</LagrangianLinearTIDS>
		</DS_Definition>
		<Interaction_Definition>
			<Interaction number='1' Id='test-of-rel'>
				<size> 1 </size>
				<!-- List of DS concerned by this interaction -->
		               <DS_Concerned all='true'>
                               </DS_Concerned>									
				<!-- Relation of this interaction -->				
				<Interaction_Content>		
					<FirstOrderRelation>
					<g plugin="TestPlugin:R"/>
                                        <jacobianG number='1'>
					    <matrix matrixPlugin="TestPlugin:Jg0"/>
                                        </jacobianG>
					<h plugin="TestPlugin:y"/>
                                        <jacobianH number='2'>
					    <matrix matrixPlugin="TestPlugin:Jh0"/>
					    <matrix matrixPlugin="TestPlugin:Jh1"/>
                                        </jacobianH>
					</FirstOrderRelation>			
					<!-- NS Law of this interaction	-->
					<NewtonImpactLaw>
						<e>0.9</e>				
					</NewtonImpactLaw>
				</Interaction_Content>	
			</Interaction>				
		</Interaction_Definition>		
	</NSDS>
</SiconosModel>
