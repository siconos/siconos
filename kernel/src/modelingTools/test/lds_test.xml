				<A matrixRowSize='3' matrixColSize='3'>	
					<row>2.0 2.0 2.0</row>
					<row>4.0 4.0 4.0</row>
					<row>6.0 6.0 6.0</row>
				</A>			
			</FirstOrderLinearDS>
			<FirstOrderLinearDS number='2'>
				<x0 vectorSize='3'>
					2.0 4.0 6.0
				</x0>
				<b vectorPlugin="TestPlugin:computeb"/>
				<A matrixPlugin="TestPlugin:computeA"/>	
 			</FirstOrderLinearDS>
		</DS_Definition>
	</NSDS>
</SiconosModel>
